{
  "Serilog": {
    "Using": ["Serilog.Sinks.File", "Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/applogv1-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithThreadId", "WithMachineName"]
  },
  "JwtSettings": {
    "validIssuer": "Presentation",
    "validAudience": "localhost:5043",
    "expires": 15,
    "RefreshTokenExpiryDays": 7
  },
  "GeminiSettings": {
    "ApiKey": "AIzaSyBmG4-bv2cbmOGgKIPgp3arwkmCM3Dww7M"
  },
  "AllowedHosts": "*"
}
